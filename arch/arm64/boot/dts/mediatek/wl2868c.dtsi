&odm {
	wl2868c_eint:wl2868c_eint {
	};

	wl2868c_gpio1: wl2868c-gpio1 {
		compatible = "regulator-fixed";
		regulator-name = "wl2868c-gpio1";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&wl2868c_ldo1>;
		enable-active-high;
		gpio = <&wl2868c 0 0x0>;
	};

	wl2868c_gpio2: wl2868c-gpio2 {
		compatible = "regulator-fixed";
		regulator-name = "wl2868c-gpio2";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&wl2868c_ldo1>;
		enable-active-high;
		gpio = <&wl2868c 1 0x0>;
	};

	wl2868c_gpio3: wl2868c-gpio3 {
		compatible = "regulator-fixed";
		regulator-name = "wl2868c-gpio3";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&wl2868c_ldo1>;
		enable-active-high;
		gpio = <&wl2868c 2 0x0>;
	};
};

&i2c2 {
	wl2868c: wl2868c@2F {
		status = "ok";
		compatible = "willsemi,wl2868c";
		reg = <0x2F>;
		wakeup-source;
		/* Temporarily choose unused gpios */
		interrupts-extended = <&pio 117 0x0>;
		reset-gpio = <&pio 159 0x0>;
		dcdc-gpio   = <&pio 88 0x0>;
		gpio-supply = <&wl2868c_ldo1>;
		gpio-controller;
		#gpio-cells = <2>;
		regulator_nb = "wl2868c-ldo1", "wl2868c-ldo2",
				"wl2868c-ldo3", "wl2868c-ldo4",
				"wl2868c-ldo5", "wl2868c-ldo6",
				"wl2868c-ldo7", "wl2868c-ldo8";
		regulators {
			BASE {
				regulator-name = "wl2868c,base";
				oc_shutdown_all = <0>;
				pgb_shutdown_all = <0>;
			};
			wl2868c_ldo1: LDO1 {
				regulator-name = "wl2868c-ldo1";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo2: LDO2 {
				regulator-name = "wl2868c-ldo2";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo3: LDO3 {
				regulator-name = "wl2868c-ldo3";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo4: LDO4 {
				regulator-name = "wl2868c-ldo4";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo5: LDO5 {
				regulator-name = "wl2868c-ldo5";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo6: LDO6 {
				regulator-name = "wl2868c-ldo6";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			wl2868c_ldo7: LDO7 {
				regulator-name = "wl2868c-ldo7";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
		};
	};
};
