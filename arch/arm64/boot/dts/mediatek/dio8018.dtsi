&odm {
	dio8018_eint:dio8018_eint {
	};

	dio8018_gpio1: dio8018-gpio1 {
		compatible = "regulator-fixed";
		regulator-name = "dio8018-gpio1";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&dio8018_ldo1>;
		enable-active-high;
		gpio = <&dio8018 0 0x0>;
	};

	dio8018_gpio2: dio8018-gpio2 {
		compatible = "regulator-fixed";
		regulator-name = "dio8018-gpio2";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&dio8018_ldo1>;
		enable-active-high;
		gpio = <&dio8018 1 0x0>;
	};

	dio8018_gpio3: dio8018-gpio3 {
		compatible = "regulator-fixed";
		regulator-name = "dio8018-gpio3";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&dio8018_ldo1>;
		enable-active-high;
		gpio = <&dio8018 2 0x0>;
	};
};

&i2c2 {
	dio8018: dio8018@35 {
		status = "ok";
		compatible = "willsemi,dio8018";
		reg = <0x35>;
		wakeup-source;
		/* Temporarily choose unused gpios */
		interrupts-extended = <&pio 117 0x0>;
		reset-gpio = <&pio 159 0x0>;
		dcdc-gpio   = <&pio 88 0x0>;
		gpio-supply = <&dio8018_ldo1>;
		gpio-controller;
		#gpio-cells = <2>;
		regulator_nb = "dio8018-ldo1", "dio8018-ldo2",
				"dio8018-ldo3", "dio8018-ldo4",
				"dio8018-ldo5", "dio8018-ldo6",
				"dio8018-ldo7", "dio8018-ldo8";
		regulators {
			BASE {
				regulator-name = "dio8018,base";
				oc_shutdown_all = <0>;
				pgb_shutdown_all = <0>;
			};
			dio8018_ldo1: LDO1 {
				regulator-name = "dio8018-ldo1";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo2: LDO2 {
				regulator-name = "dio8018-ldo2";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo3: LDO3 {
				regulator-name = "dio8018-ldo3";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo4: LDO4 {
				regulator-name = "dio8018-ldo4";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo5: LDO5 {
				regulator-name = "dio8018-ldo5";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo6: LDO6 {
				regulator-name = "dio8018-ldo6";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <3000000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
			dio8018_ldo7: LDO7 {
				regulator-name = "dio8018-ldo7";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <1200000>;
				regulator-active-discharge;
				oc_ptsel = <1>;
				pgb_ptsel = <1>;
				soft_start_time_sel = <1>;
			};
		};
	};
};
